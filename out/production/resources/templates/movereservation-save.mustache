{{>layout/header}}

<h1>예약화면</h1>

<div class="col-md-12">
    <div class="col-md-4">
        <form>
            <div class="form-group">
                <label for="dong">예약 동</label>
                <input type="text" class="form-control" id="dong" placeholder="예약 동을 입력하세요">
            </div>
            <div class="form-group">
                <label for="ho">예약 호수</label>
                <input type="text" class="form-control" id="ho" placeholder="예약 호수를 입력하세요">
            </div>
            <div class="form-group">
                <label for="moveDate">이사예약일자</label>
                <textarea class="form-control" id="moveDate" placeholder="이사예약일자를 입력하세요"></textarea>
            </div>
        </form>
        <a href="/" role="button" class="btn btn-secondary">취소</a>
        <button type="button" class="btn btn-primary" id="btn-save">예약등록</button>
    </div>
</div>

{{>layout/footer}}